{
  "presets": [
    "react"
  ],
  "plugins": [
    "dynamic-import-node",
    "transform-class-properties",
    "transform-object-rest-spread",
    ["module-resolver", {
      "root": ["./"],
      "alias": {
        "Client": "./client",
        "Server": "./server",
        "Shared": "./shared",
        "Public": "./public"
      }
    }]
  ]
}
