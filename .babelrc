{
  "presets": [
    "react",
    ["env", {
      "targets": {
        "browsers": ["last 2 versions", "safari >= 7"],
        "node": "9"
      }
    }]
  ],
  "plugins": [
    "dynamic-import-node",
    "transform-class-properties",
    "transform-object-rest-spread",
    ["module-resolver", {
      "root": ["./"],
      "alias": {
        "Client": "./client",
        "Server": "./server",
        "Shared": "./shared",
        "Public": "./public"
      }
    }]
  ]
}
